{
  "id": "kafkalot",
  "version": "0.0.1",
  "description": "dashboard for kafka-connect",
  "scripts": {
    "postinstall": "bower cache clean && bower install",
    "open:dev": "babel-node tools/devServer.js",
    "open:prod": "babel-node tools/prodServer.js",
    "prestart": "npm run remove-dist",
    "start:mock-kafka-rest-server": "babel-node tools/mockServers/runKafkaRest",
    "start:mock-storage-server": "babel-node tools/mockServers/runStorage",
    "start:client": "cross-env NODE_ENV=development npm-run-all --parallel lint:tools test:watch open:dev",
    "start": "npm-run-all --parallel start:mock-storage-server start:client",
    "lint:src": "eslint src",
    "lint:tools": "eslint webpack tools",
    "lint": "npm run lint:src && npm run lint:tools",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "build:js": "cross-env NODE_ENV=production babel-node tools/build.js",
    "prebuild": "npm run clean-dist && npm run build:html",
    "build": "npm test && npm run build:js",
    "build-run": "npm run build && npm run open:prod",
    "test": "cross-env NODE_ENV=test mocha --reporter progress --compilers js:babel-core/register --recursive \"./src/**/*.spec.js\" --require ignore-styles",
    "test:watch": "npm run test -- --watch",
    "coverage:open": "cross-env NODE_ENV=test  istanbul cover node_modules/.bin/_mocha -- -u exports --compilers js:babel-register --recursive \"./src/**/*.spec.js\" --require ignore-styles && open ./coverage/lcov-report/index.html",
    "coverage:coverall": "cross-env NODE_ENV=test  istanbul cover node_modules/.bin/_mocha -- -u exports --compilers js:babel-register --recursive \"./src/**/*.spec.js\" --require ignore-styles --report lcovonly -- && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "author": "1ambda@gmail.com",
  "license": "Apache-2.0",
  "dependencies": {
    "connect-history-api-fallback": "1.2.0",
    "debug": "2.2.0",
    "deep-equal": "1.0.1",
    "fs-extra": "0.30.0",
    "isomorphic-fetch": "2.2.1",
    "jsoneditor": "5.5.5",
    "keycode": "2.1.1",
    "material-ui": "0.15.0",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "react-redux": "4.4.5",
    "react-router": "2.4.1",
    "react-router-redux": "4.0.4",
    "react-tap-event-plugin": "1.0.0",
    "redux": "3.5.2",
    "redux-saga": "0.10.4"
  },
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "babel-cli": "6.9.0",
    "babel-core": "6.9.0",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "body-parser": "1.15.1",
    "bower": "1.7.9",
    "browser-sync": "2.12.10",
    "chai": "3.5.0",
    "cheerio": "0.20.0",
    "colors": "1.1.2",
    "cors": "2.7.1",
    "coveralls": "2.11.9",
    "cross-env": "1.0.8",
    "css-loader": "0.23.1",
    "eslint": "2.11.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "5.1.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "http-proxy-middleware": "0.15.1",
    "ignore-styles": "2.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "lowdb": "0.13.0-beta-5",
    "main-bower-files": "2.13.1",
    "mocha": "2.5.3",
    "mocha-lcov-reporter": "1.2.0",
    "npm-run-all": "2.1.1",
    "postcss-cssnext": "2.5.2",
    "postcss-import": "8.1.2",
    "postcss-loader": "0.9.1",
    "postcss-reporter": "1.3.3",
    "postcss-url": "5.1.2",
    "react-paginate": "2.0.1",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redbox-react": "1.2.6",
    "redux-actions": "0.9.1",
    "rimraf": "2.5.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "watch": "0.18.0",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0",
    "yargs": "4.7.1"
  },
  "keywords:": [
    "kafka",
    "connect",
    "dastboard",
    "kafkalot"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/1ambda/kafka-connect-dashboard"
  }
}
